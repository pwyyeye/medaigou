  </attribute>
  <attribute>
   <name>formString</name>
   <rtexprvalue>true</rtexprvalue>
   <description>定义Form的内容,eg:{
					xtype:'textfield',
					width : 200,
					name : 'name',
					fieldLabel:'角色'
				},{xtype:'combo',width : 200,
					allowBlank : false,
					blankText : '必须选择所属模块',
					hiddenName : 'moduleID',
					name : 'module',					
				    store:new Ext.data.JsonStore({data:moduleData,fields:["id","name"]}),
					editable : false,
					mode: 'local',    
					triggerAction: 'all',
					displayField:'name',
					valueField : 'id',
					emptyText :'请选择所属模块',
					fieldLabel:'模块'				   
				}}</description>
  </attribute>
  <attribute>
   <name>searchFormString</name>
   <rtexprvalue>true</rtexprvalue>
   <description>定义Form的内容,eg:{
					xtype:'textfield',
					width : 200,
					name : 'name',
					fieldLabel:'角色'
				},{xtype:'combo',width : 200,
					allowBlank : false,
					blankText : '必须选择所属模块',
					hiddenName : 'moduleID',
					name : 'module',					
				    store:new Ext.data.JsonStore({data:moduleData,fields:["id","name"]}),
					editable : false,
					mode: 'local',    
					triggerAction: 'all',
					displayField:'name',
					valueField : 'id',
					emptyText :'请选择所属模块',
